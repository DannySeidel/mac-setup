alias gc='git commit -m'
alias gs='git status'
alias gb='git branch'
alias ga='git add'
alias gaa='git add .'
alias gps='git push origin'
alias gpl='git pull origin'
alias gd='git diff'
alias gl='git lg'
alias glg='git log'
alias gch='git checkout'
alias gx='git reset --hard'
alias gr='git reset'
alias grs='git reset HEAD~1 --soft'
alias grh='git reset HEAD~1 --hard'

